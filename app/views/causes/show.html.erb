<div class="page-header">
  <%= link_to causes_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cause_path(@cause), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cause</h1>
</div>

<dl class="dl-horizontal">
  <dt>State:</dt>
  <dd><%= @cause.state %></dd>

  <dt>category:</dt>
  <dd><%= @cause.cause_category.name %></dd>

  <dt>Name:</dt>
  <dd><%= @cause.name %></dd>

  <dt>Website:</dt>
  <dd><%= @cause.website %></dd>

  <dt>Phone:</dt>
  <dd><%= @cause.phone %></dd>

  <dt>Email:</dt>
  <dd><%= @cause.email %></dd>

  <dt>Social contribution:</dt>
  <dd><%= @cause.social_contribution %></dd>

  <dt>One liner:</dt>
  <dd><%= @cause.one_liner %></dd>

  <dt>Address:</dt>
  <dd><%= @cause.address %></dd>

  <dt>Latitude:</dt>
  <dd><%= @cause.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @cause.longitude %></dd>

  <dt>Logo:</dt>
  <dd><%= @cause.logo %></dd>

</dl>
