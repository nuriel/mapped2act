<div class="page-header">
  <%= link_to cause_categories_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_cause_category_path(@cause_category), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show cause_category</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @cause_category.name %></dd>

  <dt>Description:</dt>
  <dd><%= @cause_category.description %></dd>

  <dt>Marker address:</dt>
  <dd><%= @cause_category.marker_address %></dd>

</dl>
